<launch>
  <arg name="paused" default="false" />
  <arg name="use_sim_time" default="true" />
  <arg name="gui" default="true" />
  <arg name="headless" default="false" />
  <arg name="debug" default="false" />
  <arg name="verbose" default="true" />

  <!-- <arg name="physics" default="ode"/> -->
  <!-- <arg name="physics" default="bullet"/> -->
  <!-- <arg name="physics" default="simbody"/> -->
  <arg name="physics" default="dart"/>

  <include file="$(find digital_twin)/launch/empty_world.launch">
    <arg name="paused" value="$(arg paused)"/>
    <arg name="use_sim_time" value="$(arg use_sim_time)"/>
    <arg name="gui" value="$(arg gui)"/>
    <arg name="headless" value="$(arg headless)"/>
    <arg name="debug" value="$(arg debug)"/>
    <arg name="physics" value="$(arg physics)"/>
    <arg name="verbose" value="$(arg verbose)"/>
  </include>

  <arg name="robot_ns" default="robot"/>

  <group ns="$(arg robot_ns)">
    <arg name="model_name" default="omwr"/>

    <include file="$(find digital_twin)/launch/import_model_xacro.launch">
      <arg name="model_name" value="$(arg model_name)"/>
    </include>

    <include file="$(find digital_twin)/launch/model_control.launch">
      <arg name="model_name" value="$(arg model_name)"/>
    </include>
  </group>

</launch>
