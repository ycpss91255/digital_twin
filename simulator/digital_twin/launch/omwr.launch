<launch>
  <arg name="robot_ns" default="/robot"/>

  <group ns="$(arg robot_ns)">
    <arg name="model_name" default="omwr"/>

    <include file="$(find digital_twin)/launch/import_model.launch">
      <arg name="model_name" value="$(arg model_name)"/>
      <arg name="robot_ns" value="$(arg robot_ns)"/>
    </include>

    <include file="$(find digital_twin)/launch/model_control.launch">
      <arg name="model_name" value="$(arg model_name)"/>
      <arg name="robot_ns" value="$(arg robot_ns)"/>
    </include>
  </group>

</launch>
