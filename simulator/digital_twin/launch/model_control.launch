<?xml version="1.0" encoding="utf-8"?>
<launch>
<arg name="robot_ns" default="/digital/robot"/>

  <!-- <group ns="$(arg robot_ns)"> -->
    <arg name="model_name" default="omwr"/>
    <rosparam file="$(find model)/$(arg model_name)/config/$(arg model_name).yaml" command="load" ns="$(arg robot_ns)"/>
    <!-- load the controllers -->
    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false" output="screen"
    args=" --namespace $(arg robot_ns)
          joint_state_controller
          wheel1
          wheel2
          wheel3
          wheel4">
    </node>

    <!-- convert joint states to TF transforms for rviz, etc -->
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="false" output="screen">
      <!-- <remap from="/dog" to="/dog/rviz"/> -->
    </node>
    <!-- <node pkg="digital_twin" type="node_handle_test" name="node_handle_test" output="screen" respawn="false" respawn_delay="0"/> -->
  <!-- </group> -->
</launch>
